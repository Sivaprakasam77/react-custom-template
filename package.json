{
  "name": "react-custom-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:dev": "tsc && vite build --mode development && vite preview",
    "build:prod": "tsc && vite build && vite preview",
    "deploy:rsync": "rsync -avP -e ssh dist/* USRENAME@SERVER_IP:/FOLDER_PATH_ON_SERVER",
    "deploy:rsync:key": "rsync -avP -e 'ssh -i KEYFILE.pem' dist/* USRENAME@SERVER_IP:/FOLDER_PATH_ON_SERVER",
    "deploy:scp": "scp -r dist/* USRENAME@SERVER_IP:/FOLDER_PATH_ON_SERVER",
    "deploy:scp:key": "scp -i .pem -r dist/* USRENAME@SERVER_IP:/FOLDER_PATH_ON_SERVER",
    "deploy:firebase": "firebase deploy",
    "app:init": "npx cap init",
    "app:clean": "rm -rd android ios",
    "ionic:build": "rm -rd android ios && tsc && vite build && npx cap add android && npx cap add ios"
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.114",
    "@mui/material": "^5.11.2",
    "@mui/x-date-pickers": "^5.0.12",
    "axios": "^1.2.2",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.29.3",
    "draft-js": "^0.11.7",
    "firebase": "^9.15.0",
    "formik": "^2.2.9",
    "jest": "^29.3.1",
    "numeral": "^2.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-draft-wysiwyg": "^1.15.0",
    "react-easy-crop": "^4.6.3",
    "react-error-boundary": "^3.1.4",
    "react-number-format": "^5.1.2",
    "react-query": "^3.39.2",
    "react-router-dom": "^6.6.1",
    "vite-plugin-pwa": "^0.14.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@capacitor/android": "^4.6.1",
    "@capacitor/cli": "^4.6.1",
    "@capacitor/core": "^4.6.1",
    "@capacitor/ios": "^4.6.1",
    "@types/draft-js": "^0.11.10",
    "@types/numeral": "^2.0.2",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-draft-wysiwyg": "^1.13.4",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0"
  }
}
